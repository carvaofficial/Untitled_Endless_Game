<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.carva.untitledendlessgame">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".Scenes.Achievments.AchievementsActivity"></activity>
        <activity android:name=".Scenes.Markers.MarkersActivity" />
        <activity android:name=".Games.GameOverActivity" />
        <activity android:name=".Scenes.CreditsActivity" />
        <activity
            android:name=".Games.Game1Activity"
            android:screenOrientation="portrait" />
        <activity android:name=".Scenes.Tutorials.TutorialGameMode2" />
        <activity android:name=".Scenes.Tutorials.TutorialGameMode1" />
        <activity android:name=".Scenes.SettingsActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>